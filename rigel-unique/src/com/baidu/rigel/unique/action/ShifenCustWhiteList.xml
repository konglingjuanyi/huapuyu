<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <package name="shifenCustWhiteList" extends="rigel-default" namespace="/shifenCustWhiteList">
         
        <action name="shifenCustWhitelist" class="com.baidu.rigel.unique.action.ShifenCustWhitelistAction" method="queryShifenCustWhitelist">
            <interceptor-ref name="baseStack"/>
            <!-- <interceptor-ref name="researchInterceptor"/> -->
            <result name="success" type="velocity">/shifenCustWhiteList/shifenCustWhitelist.vm</result>
        </action>
        
        <action name="shifenCustWhitelistSearch" class="com.baidu.rigel.unique.action.ShifenCustWhitelistAction" method="queryShifenCustWhitelist">
            <interceptor-ref name="baseStack"/>
            <!-- <interceptor-ref name="researchInterceptor"/> -->
            <result name="success" type="velocity">/shifenCustWhiteList/shifenCustWhitelist.vm</result>
        </action>
        
        <action name="shifenCustWhitelistAdd" class="com.baidu.rigel.unique.action.ShifenCustWhitelistAction" method="addShifenCustWhitelist">
            <interceptor-ref name="baseStack"/>
            <!-- <interceptor-ref name="researchInterceptor"/> -->
            <result name="success" type="velocity">/shifenCustWhiteList/shifenCustWhitelist.vm</result>
        </action>
        
        <action name="shifenCustWhitelistDel" class="com.baidu.rigel.unique.action.ShifenCustWhitelistAction" method="delShifenCustWhitelist">
            <interceptor-ref name="baseStack"/>
            <!-- <interceptor-ref name="researchInterceptor"/> -->
            <result name="success" type="velocity">/shifenCustWhiteList/shifenCustWhitelist.vm</result>
        </action>
        
         <action name="shifenCustWhitelistDeptlist" class="com.baidu.rigel.unique.action.ShifenCustWhitelistAction" method="deptlist">
            <interceptor-ref name="baseStack"/>
            <!-- <interceptor-ref name="researchInterceptor"/> -->
            <result name="success" type="velocity">/shifenCustWhiteList/listOpt.json.vm</result>
        </action>
    </package>
	
</struts>