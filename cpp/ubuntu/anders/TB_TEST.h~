#include <iostream>
#include <string>
using namespace std;

#include <config.h>

class TB_TEST
{
public:
	void setId(string id)
	{
		this->id = id;
	}
	string getId()
	{
		return this->id;
	}
	int getIdType()
	{
		return TYPE_STRING;
	}
	void setName(string name)
	{
		this->name = name;
	}
	string getName()
	{
		return this->name;
	}
	int getNameType()
	{
		return TYPE_STRING;
	}
private:
	string id;
	string name;
};
