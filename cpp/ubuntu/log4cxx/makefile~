objects=test.o

acelib=-L/home/anders/software/ACE_wrappers/lib -lACE

aceinc=-I/home/anders/software/ACE_wrappers/

test : $(objects)
	g++ -o test $(objects) $(acelib)

test.o : test.cpp
	g++ -c $(aceinc) test.cpp
#make具有自动推导能力，因此上面的操作完全可以用下面的操作代替
#test.o : otlv4.h

.PHONY : clean
clean : 
	-rm test $(objects)
