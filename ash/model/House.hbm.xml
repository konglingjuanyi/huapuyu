<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="model" assembly="model">
	
  <class name="House" table="tb_house">
    <id name="id" column="id" type="long">
      <generator class="sequence">
        <param name="sequence">seq_house</param>
      </generator>
    </id>

    <property name="provinceId" column="province_id" type="integer" not-null="true"/>
    <property name="cityId" column="city_id" type="integer" not-null="true"/>
    <property name="districtId" column="district_id" type="integer" not-null="true"/>
    <property name="address" column="address" type="string" not-null="true"/>
    <property name="bedroomCount" column="bedroom_count" type="integer" not-null="true"/>
    <property name="livingRoomCount" column="living_room_count" type="integer" not-null="true"/>
    <property name="kitchenCount" column="kitchen_count" type="integer" not-null="true"/>
    <property name="washroomCount" column="washroom_count" type="integer" not-null="true"/>
    <property name="balconyCount" column="balcony_count" type="integer" not-null="true"/>
    <property name="propMgtTypeId" column="prop_mgt_type_id" type="integer"/>
    <property name="directionId" column="direction_id" type="integer"/>
    <property name="decorationId" column="decoration_id" type="integer"/>
    <property name="totalFloor" column="total_floor" type="integer"/>
    <property name="floor" column="floor" type="integer"/>
    <property name="constructYearId" column="construct_year_id" type="integer"/>
    <property name="transport" column="transport" type="string"/>
    <property name="environment" column="environment" type="string"/>
    <property name="remark" column="remark" type="string"/>
    
    <one-to-one
  </class>

</hibernate-mapping>
