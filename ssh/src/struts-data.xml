<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="data" namespace="/data" extends="struts-default">

       	<action name="add" class="struts.action.DataAction" method="add">
			<result name="success">/jsp/data/list.jsp</result>
			<result name="input">/jsp/data/add.jsp</result>
		</action>
		
		<action name="edit" class="struts.action.DataAction" method="edit">
			<result name="success">/jsp/data/list.jsp</result>
			<result name="input">/jsp/data/edit.jsp</result>
		</action>
		
		<action name="load" class="struts.action.DataAction" method="load">
			<result>/jsp/data/edit.jsp</result>
		</action>
		
		<action name="list" class="struts.action.DataAction" method="list">
			<result>/jsp/data/list.jsp</result>
		</action>
		
		<action name="delete" class="struts.action.DataAction" method="delete">
			<result>/jsp/data/list.jsp</result>
		</action>
		
    </package>
</struts>
	